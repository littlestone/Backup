                    default:
                        this.XmlHelper.XMLFields[name, scope].XPaths[xpath].Values.Add(value.ToString());
                        break;
                }
            }
            else
            {
                this.XmlHelper.XMLFields[name, scope].XPaths[xpath].Values.Add(string.Empty);
            }
            #endregion
        }

