        /// Before returning from this method, set the value of Dts.TaskResult to indicate success or failure.
        /// To open Help, press F1.
        /// </summary>
        public void Main()
        {
            // TODO: Add your code here

            Boolean the_fire_again = true;

            try
            {
                string strAssetsRootPath = Dts.Variables["AssetsRootPath"].Value.ToString();
                string strAssetResourceFilePath = Dts.Variables["AssetResourceFilePath"].Value.ToString();
                string strMainAssetPushQueuePath = Dts.Variables["MainAssetPushQueuePath"].Value.ToString();
                string strTargetRootDirPath = Dts.Variables["TargetRootDirPath"].Value.ToString();
                string strSourceSubDirName = "", strSourceFileName = "", strSourceFileFullPath = "", textLine = "";
                string strTargetSubDirName = "", strTargetDirPath = "", strTargetFileFullPath = "", strTargetFileName = "";

                // Initialization                
                if (Directory.Exists(strTargetRootDirPath)) Directory.Delete(strTargetRootDirPath, true);
                if (!Directory.Exists(strTargetRootDirPath)) Directory.CreateDirectory(strTargetRootDirPath);

                // Copy per asset resource from source to destination
                if (File.Exists(strAssetResourceFilePath))
                {
                    StreamReader objReader;
                    objReader = new StreamReader(strAssetResourceFilePath, Encoding.GetEncoding("iso-8859-1"));
                    do
                    {
                        // Read context line from asset resource data csv file
                        textLine = objReader.ReadLine();
                        strSourceSubDirName = textLine.Split(',')[0].ToString();
                        strSourceFileName = textLine.Split(',')[1].ToString();
                        strTargetFileName = textLine.Split(',')[2].ToString();
                        if (strSourceSubDirName == "" || strSourceFileName == "" || strTargetFileName == "")
                        {
                            Dts.Events.FireInformation(0, "Warning: ", "Skipped -> {" + textLine + "}", string.Empty, 0, ref the_fire_again);
                            continue;
                        }

                        // Retrieve source asset fileinfo      
                        strSourceFileFullPath = strMainAssetPushQueuePath + strSourceSubDirName + @"\" + strSourceFileName;
                        Dts.Events.FireInformation(0, "Source: ", strSourceFileFullPath + " " + strSourceFileFullPath.Length, string.Empty, 0, ref the_fire_again);

                        // Retrieve target asset fileinfo
                        strTargetSubDirName = strSourceSubDirName;
                        strTargetDirPath = strTargetRootDirPath + strTargetSubDirName;
                        strTargetFileFullPath = strTargetDirPath + @"\" + strTargetFileName;
                        Dts.Events.FireInformation(0, "Target: ", strTargetFileFullPath + " " + strTargetFileFullPath.Length, string.Empty, 0, ref the_fire_again);

                        // Create asset sub-directory if not exists
                        if (!Directory.Exists(strTargetDirPath)) Directory.CreateDirectory(strTargetDirPath);

                        // Copy asset file from source to target
                        if (!File.Exists(strTargetFileFullPath))
                        {
                            File.Copy(strSourceFileFullPath, strTargetFileFullPath);
                        }
                        else  // This will check for the existence of files with tempFileName and increment the number by one until it finds a name that does not exist in the directory
                        {
                            int count = 1;

                            string fileNameOnly = Path.GetFileNameWithoutExtension(strTargetFileFullPath);
                            string extension = Path.GetExtension(strTargetFileFullPath);
                            string path = Path.GetDirectoryName(strTargetFileFullPath);
                            string newFullPath = strTargetFileFullPath;

                            while (File.Exists(newFullPath))
                            {
                                string tempFileName = string.Format("{0}({1})", fileNameOnly, count++);
                                newFullPath = Path.Combine(path, tempFileName + extension);
                            }

                            File.Copy(strSourceFileFullPath, newFullPath);
                        }
                    } while (objReader.Peek() != -1);
                    objReader.Close();

                    // Clean up
                    if (File.Exists(strAssetResourceFilePath)) File.Delete(strAssetResourceFilePath);
                    Dts.Events.FireInformation(0, "Info: ", "Clean up completed successfuly.", string.Empty, 0, ref the_fire_again);

                    // log
                    Dts.Events.FireInformation(0, "Info: ", "End process!", string.Empty, 0, ref the_fire_again);

                    // standard
                    Dts.TaskResult = (int)ScriptResults.Success;
                }
                else
                {
                    throw new Exception("Asset resource file not found { " + strAssetResourceFilePath + " }.");
                }
            }
            catch (Exception ex)
            {
                Dts.Events.FireInformation(0, "Exception: ", ex.Message, string.Empty, 0, ref the_fire_again);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

