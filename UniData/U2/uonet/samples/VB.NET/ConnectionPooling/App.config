    <system.diagnostics>
        <switches>
            <!-- Set value property of the TraceMethods switch to one of the following:
                0 (false) or 1 (true) -->
            <add name="UniBooleanSwitch" value="1" />

            <!-- Set value property of the Arithmetic switch to one of the following:
                1(error), 2(warning), 3(info), 4(verbose) -->
            <add name="UniTraceSwitch" value="3" />
        </switches>
        <trace autoflush="true" indentsize="4">
			<listeners>
				<!-- <add name="UniListener" type="System.Diagnostics.TextWriterTraceListener, UODOTNET version=1.0.* Culture=neutral" initializeData="c:\myListener.log" /> -->
				<!-- <add name="myListener" 
				     type="System.Diagnostics.TextWriterTraceListener"
				     initializeData="c:\temp\myListener.log" /> -->
				     <add name="myListener" 
				     type="System.Diagnostics.TextWriterTraceListener"
				     initializeData="c:\temp\myListener.log" /> 
				     
				     
				
			</listeners>
         </trace>

    </system.diagnostics>
</configuration>
